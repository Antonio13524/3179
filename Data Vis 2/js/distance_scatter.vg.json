{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": "container",
    "height": 500,
    "data": {"url": "https://raw.githubusercontent.com/Antonio13524/3179/main/Data%20Vis%202/data/out_no_na.csv"},

    "layer":[
      {
        "transform": [{"filter": "datum.Price < 9000000"}],
    "mark": {"type": "point",
              "size": 1},
    "encoding": {
      "x": {"field": "Distance", "type": "quantitative"},
      "y": {"field": "Price", "type": "quantitative", "aggregate": "median"},
      "tooltip": [{"field": "Suburb", "type": "nominal"},
                  {"field": "Price", "type": "quantitative", "format": "$,"},
                  {"field": "Type", "type": "nominal"},
                  {"field": "Rooms", "type": "ordinal"}]
        }
      }


    ]
    
  }