{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 450,
    "title": "UPDATE THIS",
    "projection": {"type": "albers",
                   "center": [37.8124,144.9623 ]},
    "layer": [
      {
        "data": {
          "url": "MB_2016_VIC.json",
          "format": {"type": "topojson", "feature": "MB_2016_VIC"}
        },
        "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
      },
      {
        "data": {
          "url": "MB_2016_VIC.json",
          "format": {"type": "topojson", "feature": "MB_2016_VIC"}
        },
        "mark": {"type": "geoshape", "fill": null, "stroke": "lightgray"}
      },
      {
        "data": {
          "url": "../data/out.csv"
        },
        "mark": {"type": "circle", "tooltip": {"content": "data"}},
        "encoding": {
          "longitude": {"field": "Longtitude", "type": "quantitative"},
          "latitude": {"field": "Lattitude", "type": "quantitative"},
          "size": {
            "field": "Bathroom",
            "type": "quantitative",
            "title": "Magnitude (Richter scale)",
            "scale": {"domain": [1, 8]}
          },
          "color": {
            "field": "Price",
            "type": "quantitative",
            "title": "Depth in km",
            "scale": {"scheme": "reds"}
          }
        }
      }
    ]
  }